    "@types/quill": "^2.0.10",
    "@types/react": "^18.2.7",
    "@types/react-dom": "^18.2.4",
    "ajv": "^8.11.0",
    "app-root-path": "^3.1.0",
    "axios": "^1.3.5",
    "base64-js": "^1.5.1",
    "benchmark": "^2.1.4",
