    "ws": "^8.14.2",
    "yjs": "13.6.9",
    "ywasm": "0.16.10"
  },
  "types": "lib/index.d.ts",
  "typings": "lib/index.d.ts",
  "jest": {
    "verbose": true,
    "testEnvironmentOptions": {
      "url": "http://localhost/"
    },
    "setupFiles": [
      "<rootDir>/src/__tests__/setup.js"
    ],
    "moduleFileExtensions": [
      "ts",
      "js"
    ],
    "transform": {
      "^.+\\.ts$": "ts-jest"
    },
    "transformIgnorePatterns": [],
    "testRegex": ".*/(__tests__|__jest__|demo)/.*\\.(test|spec)\\.ts$"
  },
  "prettier": {
    "arrowParens": "always",
    "printWidth": 120,
    "tabWidth": 2,
    "useTabs": false,
    "semi": true,
    "singleQuote": true,
    "trailingComma": "all",
    "bracketSpacing": false
  },
  "config": {
    "commitizen": {
      "changelog": {
        "maxMessageLength": 90,
        "scopes": [
          "",
          "demo",
          "json-binary",
          "json-brand",
          "json-cli",
          "json-clone",
          "json-crdt-patch",
