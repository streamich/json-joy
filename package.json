          "json-ot",
          "json-pack",
          "json-patch-multicore",
          "json-patch-ot",
          "json-patch",
          "json-pointer",
          "json-random",
          "json-schema",
          "json-size",
          "json-stable",
          "json-text",
          "json-type",
          "json-type-value",
          "reactive-rpc",
          "util"
        ]
      }
    }
  }
