    "pretty-quick": "^3.1.3",
    "quill-delta": "^5.0.0",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "redis-parser": "^3.0.0",
    "rimraf": "^5.0.0",
    "rope.js": "0.1.0",
